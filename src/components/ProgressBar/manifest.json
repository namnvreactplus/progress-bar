{
  "name": "ProgressBar",
  "icon": "example-thumbnail.png",
  "displayName": {
    "en": "ProgressBar",
    "ja": "プラグイン名"
  },
  "defaultWidth": 155,
  "defaultHeight": 30,
  "minWidth": 100,
  "props": [
    {
      "name": "progressValue",
      "displayName": {
        "en": "value",
        "ja": "value"
      },
      "type": "number",
      "default": 20
    },
    {
      "name": "maxValue",
      "type": "number",
      "displayName": {
        "en": "max value",
        "ja": "max value"
      },
      "default": 100
    },

    {
      "name": "backgroundColor",
      "type": "color",
      "displayName": {
        "en": "BackgroundColor",
        "ja": "BackgroundColor"
      },
      "default": "#34A0DD"
    },

    {
      "name": "borderRadius",
      "default": 22
    },
    {
      "name": "fontFamily",
      "default": "Roboto, sans-serif"
    },

    {
      "name": "backgroundColor2",
      "type": "color",
      "displayName": {
        "en": "BackgroundColor2",
        "ja": "BackgroundColor2"
      },
      "default": "#34A0DD"
    }
  ]
}
